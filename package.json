{
  "name": "repair-now",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "apps/back/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "dev:back": "turbo run dev --filter=./apps/back/*",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "dependencies": {
    "nuxt": "^3.4.3",
    "@mdi/font": "^7.2.96",
    "@pinia/nuxt": "^0.4.9",
    "pinia": "^2.0.35",
    "sass": "^1.62.1",
    "vuetify": "^3.2.2"
  },
  "devDependencies": {
    "@nuxtjs/i18n": "^8.0.0-beta.11",
    "@nuxt/devtools": "^0.4.5",
    "@types/node": "^18",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
