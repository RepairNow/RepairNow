{
  "name": "repair-now",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "apps/back/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "dev:back": "turbo run dev --filter=./apps/back/*",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "dependencies": {
    "vue": "latest",
    "vue-router": "latest",
    "vite": "latest",
    "@mdi/font": "^7.2.96",
    "pinia": "^2.0.35",
    "sass": "^1.62.1",
    "vuetify": "^3.2.2",
    "vitest": "^0.31.0",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@types/node": "^18",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "latest",
    "@vitejs/plugin-vue": "latest"
  },
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
